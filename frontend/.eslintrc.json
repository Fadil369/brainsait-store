{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "react/no-unescaped-entities": "off",
    "react/display-name": "off",
    "prefer-const": "warn",
    "no-var": "error",
    "no-console": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "@next/next/no-page-custom-font": "warn",
    "@next/next/no-img-element": "warn"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "build/",
    "dist/"
  ]
}